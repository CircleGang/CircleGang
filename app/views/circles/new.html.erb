<h1>Circles#new</h1>
<p>Find me in app/views/circles/new.html.erb</p>

<h1>新規作成</h1>
<%= form_for (@circle) do |f| %>
<%= f.label :name,"サークル名" %>
<%= f.text_field :name %>

<%= f.label :place,"活動場所" %>
<%= f.text_field :place %>

<%= f.label :bio,"サークル紹介" %>
<%= f.text_field :bio %>

<%= f.label :admin,"代表者名" %>
<%= f.text_field :admin_user_id %>

<%= f.label :category,"カテゴリー選択" %>
<%= f.text_field :content %>
<%= f.submit "登録する" %>
<% end %>
